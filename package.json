{
  "type": "module",
  "name": "webpack-frontend-template",
  "version": "1.0.4",
  "private": true,
  "description": "A starter template for Frontend projects using Typescript & Sass",
  "scripts": {
    "start": "webpack serve --env dev --open",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts)\"",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts ./src",
    "test": "jest --watch",
    "test:ci": "jest --forceExit",
    "preversion": "npm-run-all format lint",
    "postversion": "git push && git push --tags",
    "prod": "webpack --env prod",
    "build": "npm-run-all format lint prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "css-loader": "^6.7.3",
    "eslint": "^8.29.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
